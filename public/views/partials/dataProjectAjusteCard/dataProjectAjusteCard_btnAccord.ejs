<button class="accordion-button p-1" type="button" data-bs-toggle="collapse"
    data-bs-target="#panelsStayOpen-collapseOneCard<%=k %>" aria-expanded="false" aria-controls="panelsStayOpen-collapseOneCard<%=k %>">
    <ul class="list-group list-group-horizontal flex-fill">
        <li class="list-group-item list-group-item-secondary my-1">
            # <strong class="mx-2"><%= k+1 %></strong>
        </li>
        <li class="list-group-item list-group-item-secondary my-1">
            OCI#: <strong id="ociNumber<%= k %>" class="mx-2"><%= arrayVisibleOci[k].ociNumber %></strong>
        </li>
        <li class="list-group-item list-group-item-secondary my-1">
            Descripcion: <strong id="ociDescription<%= k %>"><%= arrayVisibleOci[k].ociDescription %></strong>
        </li>
        <li class="list-group-item list-group-item-secondary my-1">
            Alias: <span id="ociAlias<%= k %>" class="badge bg-info text-dark mx-1"><strong><%= arrayVisibleOci[k].ociAlias %></strong></span>
        </li>
        <li class="list-group-item list-group-item-secondary my-1">
            Status OCI:
            <% if (arrayVisibleOci[k].ociStatus) { %>
                <span class="badge rounded-pill bg-primary text-white mx-1">Activa</span>
            <% } else { %>
                <span class="badge rounded-pill bg-danger text-white mx-1">Inactiva</span>
            <% } %>
        </li>
        <li class="list-group-item list-group-item-secondary my-1">
            Due√±o de Pieza:
            <% if (arrayVisibleOci[k].ociOwner[0].legajoId !== 0) { %>
                <span id="ociDueno<%= k %>" class="badge bg-success text-light mx-1">
                    <strong><%= arrayVisibleOci[k].ociOwner[0].name %> <%= arrayVisibleOci[k].ociOwner[0].lastName %>
            <% } else { %>
                <span id="ociDueno<%= k %>" class="badge bg-danger text-light mx-1">
                    <strong>No Asignado
            <% } %>
                    </strong>
                </span>
        </li>
    </ul>
</button>
<!-- End boton accordion OCI's -->