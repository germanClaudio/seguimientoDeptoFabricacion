                                                    let largoArray = parseInt(largoArrayEtapaSegundaSegunda)
                                                    resDatoAzuladoAceros = pathToMongoOtEtapaSegundaSegunda[largoArray-1].azuladoAceros == 'sinDato' ? "S/D" : pathToMongoOtEtapaSegundaSegunda[largoArray-1].azuladoAceros
