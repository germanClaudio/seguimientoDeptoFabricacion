                                        <button id="btnSiguienteAjusteFondo<%=k%>_<%=n%>_<%=q%>" name="btnSiguienteAjusteFondo" value="<%=k%>_<%=n%>_<%=q%>" class="btn btn-light btn-sm ms-2" title="Ver siguiente" type="button"
