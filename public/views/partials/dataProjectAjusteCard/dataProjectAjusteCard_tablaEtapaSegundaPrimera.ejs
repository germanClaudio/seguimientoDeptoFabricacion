                                                    pathToMongoOtEtapaSegundaPrimera[i].tachoAjuste == 'sinDato' ? arrResTachoAjuste.push("S/D") : arrResTachoAjuste.push(pathToMongoOtEtapaSegundaPrimera[i].tachoAjuste)
