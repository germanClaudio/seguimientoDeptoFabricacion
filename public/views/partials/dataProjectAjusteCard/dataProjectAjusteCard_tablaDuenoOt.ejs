<!-- tabla Dueño de OT -->
<table id="tablaDuenoOt<%=k%>" name="tablaDuenoOt" class="table-responsive align-middle col-2 border-end border-dark" style="font-size: 10pt">
    <thead class="align-middle" style="background-color: #bbc">
        <tr>
            <th colspan="3" class="text-center pb-1"><div class="mb-3">DUEÑO OT</div></th>
        </tr>
        <tr>
            <th style="width:10vw" scope="col-1" class="text-center"># Legajo</th>
            <th style="width:45vw" scope="col-1" class="text-center">Nombre</th>
            <th style="width:45vw" scope="col-1" class="text-center">Apellido</th>
        </tr>
    </thead>
    <tbody class="table-group-divider">
        <% let largoArrayOtProject = proyecto[m].project[j].oci[k].otProject.length;
        
        for ( let n=0; largoArrayOtProject> n; n++) {
            let pathMongoOtProject = proyecto[m].project[j].oci[k].otProject[n]
            let largoArrayOtAjuste = pathMongoOtProject.otAjuste.length

            for ( let o=0; largoArrayOtAjuste > o; o++) {
                if (pathMongoOtProject.visible && pathMongoOtProject.otAjuste[o].visible) {
                    if (pathMongoOtProject.otStatus) { %>
                        <tr id="rowAjusteSelected<%=k%>_<%=n%>_<%=o%>" name="rowSelected<%=k%>_<%=n%>_<%=o%>" class="border-bottom fw-semibold fs-6" style="height: 5vh;">
                    <% } else { %>
                        <tr class="border-bottom fw-semibold fs-6" style="height: 5vh; background-color: #dedede;">
                    <% } %>
                            <td class="text-center">
                                <span id="duenoOt<%=k%>_<%=n%>_<%=o%>" class="badge rounded-pill bg-success text-white my-2 ms-1">
                                    <%= pathMongoOtProject.otAjuste[o].duenoOt[0].legajoId %>
                                </span>
                            </td>
                            <td class="text-center align-middle overflow-ellipsis">
                                <small id="nombreAjuste<%=k%>_<%=n%>_<%=o%>" class="text-dark align-middle">
                                    <%= pathMongoOtProject.otAjuste[o].duenoOt[0].name %>
                                </small>
                            </td>
                            <td class="text-center align-middle overflow-ellipsis">
                                <small id="apellidoAjuste<%=k%>_<%=n%>_<%=o%>" class="text-dark">
                                    <%= pathMongoOtProject.otAjuste[o].duenoOt[0].lastName %>
                                </small>
                            </td>
                            <td class="d-none">
                                <small id="duenoOtIdHidden<%=k%>_<%=n%>_<%=o%>"><%= pathMongoOtProject.otAjuste[o].duenoOt[0]._id %></small>
                            </td>
                        </tr>
                <% }
            }    
        } %>
    </tbody>
</table>