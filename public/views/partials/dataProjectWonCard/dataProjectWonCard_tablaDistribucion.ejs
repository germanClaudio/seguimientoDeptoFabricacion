        <tr>
            <th colspan="4" class="text-center">DISTRIBUCIÃ“N
                <% if (userInfo.admin || userInfo.area === 'fabricacion') { %>
                    <button id="posBtnAddDataOtDistribucion<%=k%>" type="button" name="btnOtDistribucion" class="btn btn-small btn-success shadow m-1 position-relative"
