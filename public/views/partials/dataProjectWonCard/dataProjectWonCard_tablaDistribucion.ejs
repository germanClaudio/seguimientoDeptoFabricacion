                                        <input type="hidden" id="resRevisionHidden<%=k%>_<%=n%>_<%=o%>_<%=q%>" value="<%= arrResRevisionBancoArmado %>">

                                        <% switch (resBancoArmado) { 
                                            case "prodismo": { %>
                                                <span id="resBancoArmado<%=k%>_<%=n%>_<%=o%>_<%=q%>" class="badge rounded-pill bg-success text-white my-2">PRODISMO</span> |
                                            <% break; }        
                                            case "terceros": { %>
                                                <span id="resBancoArmado<%=k%>_<%=n%>_<%=o%>_<%=q%>" class="badge rounded-pill bg-danger text-white my-2">TERCEROS</span> |
                                            <% break; }        
                                            case "china": { %>
                                                <span id="resBancoArmado<%=k%>_<%=n%>_<%=o%>_<%=q%>" class="badge rounded-pill bg-warning text-dark my-2">CHINA</span> |
                                            <% break; }
                                            case "noAplica": { %>
                                                <span id="resBancoArmado<%=k%>_<%=n%>_<%=o%>_<%=q%>" class="badge rounded-pill bg-info text-dark my-2">N/A</span> |
                                            <% break; }
                                            default: { %>
                                                <span id="resBancoArmado<%=k%>_<%=n%>_<%=o%>_<%=q%>" class="badge rounded-pill bg-secondary text-white my-2">S/D</span> |
                                            <% break; }
                                            } %>
                                            <span id="resRevisionBancoArmado<%=k%>_<%=n%>_<%=o%>_<%=q%>" class="badge text-dark my-2"><%= resRevisionBancoArmado %></span>
                                    </td>
                                <% }
                            } %>
                        </tr>
                    <% }
                }
            }
        }) %>
    </tbody>
</table>