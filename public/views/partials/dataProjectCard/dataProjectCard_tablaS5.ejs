                                            for (let i = 0; proyecto[m].project[j].oci[k].otProject[n].otInformation[0].otInfoSim5.length > i; i++) {
                                                if (proyecto[m].project[j].oci[k].otProject[n].otInformation[0].otInfoSim5) {
                                                    let largoArray = parseInt(proyecto[m].project[j].oci[k].otProject[n].otInformation[0].otInfoSim5.length)
                                                    var resMpEnsayada = proyecto[m].project[j].oci[k].otProject[n].otInformation[0].otInfoSim5[largoArray-1].mpEnsayada
                                                    var resRevisionMpEnsayada = proyecto[m].project[j].oci[k].otProject[n].otInformation[0].otInfoSim5[largoArray-1].revisionMpEnsayada
                                                    
                                                    if (i == 0) {
                                                        arrResMpEnsayada.push(pathToMongoDBOtInfoSim5.mpEnsayada)
                                                        arrResRevisionMpEnsayada.push(pathToMongoDBOtInfoSim5.revisionMpEnsayada)
                                                    }
                                                    else if (i >= 1 && pathToMongoDBOtInfoSim5.revisionMpEnsayada != proyecto[m].project[j].oci[k].otProject[n].otInformation[0].otInfoSim5[i-1].revisionMpEnsayada) {
                                                        arrResMpEnsayada.push(pathToMongoDBOtInfoSim5.mpEnsayada)
                                                        arrResRevisionMpEnsayada.push(pathToMongoDBOtInfoSim5.revisionMpEnsayada)
                                                    }
                                                    valueArrayResMpEnsayada = parseInt(arrResMpEnsayada.length-1)
                                                                                                
                                                } else {
                                                    var resMpEnsayada = 'S/D'
                                                    var resRevisionMpEnsayada = 0
                                                }
                                            }

                                            var resMpEnsayada = proyecto[m].project[j].oci[k].otProject[n].otInformation[0].otInfoSim5[0].mpEnsayada
                                    
                                    } else {
                                        var resMpEnsayada = "S/D"
                                        var resRevisionMpEnsayada = 0
                                        arrResMpEnsayada.push(resMpEnsayada)
                                        arrResRevisionMpEnsayada.push(resRevisionMpEnsayada)
                                    } %>

