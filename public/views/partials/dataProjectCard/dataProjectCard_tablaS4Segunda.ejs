<!-- --------- tabla S4 Segunda Parte ----------- -->
<table id="tabla4SimSegunda<%=k%>"
    class="table-responsive align-middle col-2 border-end border-dark"
    style="font-size: 10pt; width: 100%">
    <thead class="align-middle" style="background-color: #88c">
        <tr>
            <th colspan="4" class="text-center">Sim 4 (Segunda Parte)
                <% if (userInfo.admin || userInfo.area === 'ingenieria') { %>
                    <button id="posBtnAddDataS4Segunda<%=k%>" type="button" name="btn4SegundaSim" class="btn btn-small btn-success shadow m-1 position-relative"
                        style="--bs-btn-font-size: .6rem; color: var(--bs-white);" title="Agregar/Modificar Datos" value="<%=k%>">
                        <i class="fa-solid fa-pencil fa-lg" aria-hidden="true"></i>
                        <span id="spanCheckSelecMasiveSim4Seg<%=k%>" class="position-absolute top-50 start-100 translate-middle badge rounded-pill bg-danger">
                            0
                            <span class="visually-hidden">Elementos seleccionados</span>
                        </span>
                    </button>
                <% } else { %>
                    <button type="button" class="btn btn-small btn-danger shadow m-1 disabled"
                        style="--bs-btn-font-size: .6rem; color: var(--bs-white);">
                        <i class="fa-solid fa-ban fa-lg" aria-hidden="true"></i>
                    </button>
                <% } %>
            </th>
        </tr>
        <tr>
            <th style="width:23vw" scope="col-1" class="text-center">Informe | Rev</th>
            <th style="width:23vw" scope="col-1" class="text-center">Geo Cop#1 | Rev</th>
            <th style="width:23vw" scope="col-1" class="text-center">Geo Cop#2 | Rev</th>

            <% let arrayHsSim4Segunda = [], sumaHsSim4SegundaTotal = 0,
                pathMongoOtProject = proyecto[m].project[j].oci[k].otProject,
                largoArrayOtProject = parseInt(pathMongoOtProject.length);
                
                for ( let n=0; largoArrayOtProject > n; n++) {
                    if (pathMongoOtProject[n].otInformation[0].otInfoSim4Segunda.length > 0 ) {
                        let largoArrayHsSim4Segunda = pathMongoOtProject[n].otInformation[0].otInfoSim4Segunda.length
                        arrayHsSim4Segunda.push(parseInt(pathMongoOtProject[n].otInformation[0].otInfoSim4Segunda[largoArrayHsSim4Segunda-1].horasSim))
                    } else {
                        arrayHsSim4Segunda.push(0)
                    }
                    sumaHsSim4SegundaTotal = arrayHsSim4Segunda.reduce(function(acumulador, valorActual) {
                        return acumulador + valorActual;
                    }, 0);
                } %>
            <th style="width:31vw" scope="col-1" class="text-center">Hs Sim | Rev | Total Hs
                <span id="resHsTotalSim4Segunda<%=k%>" class="badge rounded-pill bg-dark text-white"><%= sumaHsSim4SegundaTotal %></span>
            </th>
        </tr>
    </thead>
    <tbody class="table-group-divider">
        
        <% const sim4SegundaConfig = {
            ok: { text: "OK", classes: "bg-success text-white" },
            noOk: { text: "No OK", classes: "bg-danger text-white" },
            pendiente: { text: "Pendiente", classes: "bg-warning text-dark" },
            noAplica: { text: "N/A", classes: "bg-info text-dark" },
            default: { text: "S/D", classes: "bg-secondary text-white" }
        };

        proyecto[m].project[j].oci[k].otProject.forEach((ot, n) => {
            let pathMongoOtInformation = pathMongoOtProject[n].otInformation;

            let anteriorDisabled = "disabled", siguienteDisabled = "disabled",
                resInforme4Sim = 'sinDato', resRevisionInforme4Sim = 0,
                resGeo1Copiado = 'sinDato', resRevisionGeo1Copiado = 0,
                resGeo2Copiado = 'sinDato', resRevisionGeo2Copiado = 0,
                resHorasSim = 0, resRevisionHorasSim = 0

            if (pathMongoOtProject[n].visible) {
                let pathMongoOtSim4Segunda = pathMongoOtInformation[0].otInfoSim4Segunda || [],
                largoArraySim4Segunda = parseInt(pathMongoOtSim4Segunda.length)

                let arrResInforme4Sim = [], valueArrayResInforme4Sim = 0, arrResRevisionInforme4Sim = [], arrResCreadorInforme4Sim = [],
                arrResModificadorInforme4Sim = [], arrResFechaInforme4Sim = [], arrResFechaModInforme4Sim = [];

                let arrResGeo1Copiado = [], valueArrayResGeo1Copiado = 0, arrResRevisionGeo1Copiado = [], arrResCreadorGeo1Copiado = [],
                arrResModificadorGeo1Copiado = [], arrResFechaGeo1Copiado = [], arrResFechaModGeo1Copiado = [];

                let arrResGeo2Copiado = [], valueArrayResGeo2Copiado = 0, arrResRevisionGeo2Copiado = [], arrResCreadorGeo2Copiado = [],
                arrResModificadorGeo2Copiado = [], arrResFechaGeo2Copiado = [], arrResFechaModGeo2Copiado = [];

                let arrResHorasSim = [], valueArrayResHorasSim = 0, arrResRevisionHorasSim = [], arrResCreadorHorasSim = [],
                arrResModificadorHorasSim = [], arrResFechaHorasSim = [], arrResFechaModHorasSim = [];

                if (pathMongoOtProject[n].otStatus) { %>
                    <tr name="rowSelected<%=k%>_<%=n%>" style="height: 7vh;">
                        <% for (let q = 33; totalColDataSim4Segunda > q; q++) {
                            if (q==33) { %>
                                <!-- Informe Sim 4 data Activo--> 
                                <td class="text-center align-middle">
                                    <% if (largoArraySim4Segunda > 0) {
                                        
                                        for (let i = 0; largoArraySim4Segunda > i; i++) {
                                            if (pathMongoOtSim4Segunda[i]) {
                                                let largoArray = parseInt(largoArraySim4Segunda) 
                                                resInforme4Sim = pathMongoOtSim4Segunda[largoArray-1].informeSim4 || 'sinDato'
                                                resRevisionInforme4Sim = parseInt(pathMongoOtSim4Segunda[largoArray-1].revisionInformeSim4) || 0
                                                
                                                arrResInforme4Sim.push(pathMongoOtSim4Segunda[i].informeSim4)
                                                arrResRevisionInforme4Sim.push(pathMongoOtSim4Segunda[i].revisionInformeSim4)
                                            
                                                arrResCreadorInforme4Sim.push(pathMongoOtSim4Segunda[i].creator[0].username)
                                                arrResModificadorInforme4Sim.push(pathMongoOtSim4Segunda[i].modificator[0].username)
                                                arrResFechaInforme4Sim.push(pathMongoOtSim4Segunda[i].timestamp)
                                                arrResFechaModInforme4Sim.push(pathMongoOtSim4Segunda[i].modifiedOn)
                                            }
                                        }
                                        valueArrayResInforme4Sim = parseInt(arrResInforme4Sim.length-1)
                                                
                                        if (arrResInforme4Sim.length == 1 ) {
                                            anteriorDisabled = "disabled"
                                            siguienteDisabled = "disabled"
                                        } else if (arrResInforme4Sim.length == arrResRevisionInforme4Sim.length) {
                                            anteriorDisabled = ""
                                            siguienteDisabled = "disabled"
                                        } else {
                                            anteriorDisabled = ""
                                            siguienteDisabled = "disabled"
                                        }
                                                
                                    } else {
                                        arrResInforme4Sim.push(resInforme4Sim)
                                        arrResRevisionInforme4Sim.push(resRevisionInforme4Sim)

                                        arrResCreadorInforme4Sim.push(pathMongoOtProject[n].creator[0].username)
                                        arrResModificadorInforme4Sim.push(pathMongoOtProject[n].modificator[0].username)
                                        arrResFechaInforme4Sim.push(pathMongoOtProject[n].timestamp)
                                        arrResFechaModInforme4Sim.push(pathMongoOtProject[n].modifiedOn)
                                    } %>
                                    
                                    <div id="btnAnteriorSiguienteInforme4Sim<%=k%>_<%=n%>_<%=q%>">
