                                                arrResFechaModInfoModelo.push(pathToMongoDBOtInfo80[i].modifiedOn)
                                            }
                                        }
                                        valueArrayResInfoModelo = parseInt(arrResInfoModelo.length-1)
                                        
                                        if (arrResInfoModelo.length == 1 ) {
                                            anteriorDisabled = "disabled"
                                            siguienteDisabled = "disabled"
                                        } else if (arrResInfoModelo.length == arrResRevisionInfoModelo.length) {
                                            anteriorDisabled = ""
                                            siguienteDisabled = "disabled"
                                        } else {
                                            anteriorDisabled = ""
                                            siguienteDisabled = "disabled"
                                        }

                                    } else {
                                        arrResInfoModelo.push(resInfoModelo)
                                        arrResRevisionInfoModelo.push(resRevisionInfoModelo)
                                    } %>

                                    <div id="btnAnteriorSiguienteInfoModelo<%=k%>_<%=n%>_<%=q%>">
