<!-- --------- tabla Info al 80% ----------- -->
<table id="tablaInfo80<%=k%>"
    class="table-responsive align-middle col-5 border-end border-dark" style="font-size: 10pt; width: 100%">
    <thead class="align-middle" style="background-color: #aac">
        <tr>
            <th colspan="4" class="text-center">INFO 80%
                <% if (userInfo.admin || userInfo.area === 'ingenieria') { %>
                    <button id="posBtnAddDataInfo80<%=k%>" type="button" name="btnInfo80" class="btn btn-small btn-success shadow m-1 position-relative me-5"
                        style="--bs-btn-font-size: .6rem; color: var(--bs-white);"
                        title="Agregar/Modificar Datos" value="<%=k%>">
                        <i class="fa-solid fa-pencil fa-lg" aria-hidden="true"></i>
                        <span id="spanCheckSelecMasiveInfo80<%=k%>" class="position-absolute top-100 start-100 translate-middle badge rounded-pill bg-danger">
                            0
                            <span class="visually-hidden">Elementos seleccionados</span>
                        </span>
                    </button>
                <% } else { %>
                    <button type="button" class="btn btn-small btn-danger m-1 me-5 disabled"
                        style="--bs-btn-font-size: .6rem; color: var(--bs-white);">
                        <i class="fa-solid fa-ban fa-lg" aria-hidden="true"></i>
                    </button>
                <% } %>
                <i class="fa-solid fa-circle-check fa-lg" style="color: #067e00;"></i> Terminado -
                <i class="fa-solid fa-spinner fa-lg" style="color: #970000f5;"></i> No iniciado -
                <i class="fa-solid fa-person-digging fa-lg" style="color: #080183;"></i> En Proceso
            </th>
        </tr>
        <tr>
            <th style="width:30vw" scope="col-1" class="text-center pb-2">LDM Avan. (Cil/Guia) | Rev</th>
            <th style="width:30vw" scope="col-1" class="text-center pb-2">LDM Avan. (Tacos D2) | Rev</th>
            <th style="width:20vw" scope="col-1" class="text-center pb-2">LDM 80% | Rev</th>
            <th style="width:20vw" scope="col-1" class="text-center pb-2">Info Modelo | Rev</th>
        </tr>
    </thead>
    <tbody class="table-group-divider">

        <% let anteriorDisabled = "disabled", siguienteDisabled = "disabled",
            resLdmAvanceCG = 'sinDato', resRevisionLdmAvanceCG = 0,
            resLdmAvanceTD2 = 'sinDato', resRevisionLdmAvanceTD2 = 0,
            res80Ldm = 0, resRevision80Ldm = 0,
            resInfoModelo = 0, resRevisionInfoModelo = 0
            
        const Info80Config = {
            ok: { text: "OK", classes: "bg-success text-white" },
            noOk: { text: "No OK", classes: "bg-danger text-white" },
            pendiente: { text: "Pendiente", classes: "bg-warning text-dark" },
            noAplica: { text: "N/A", classes: "bg-info text-dark" },
            default: { text: "S/D", classes: "bg-secondary text-white" }
        };

        proyecto[m].project[j].oci[k].otProject.forEach((ot, n) => {
            let pathMongoOtProject = proyecto[m].project[j].oci[k].otProject,
            largoArrayOtProject = parseInt(pathMongoOtProject.length),
            pathMongoOtInformation = pathMongoOtProject[n].otInformation

            if (pathMongoOtProject[n].visible) {
                let pathMongoOtInfo80 = pathMongoOtInformation[0].otInfo80 || [],
                largoArrayInfo80 = parseInt(pathMongoOtInfo80.length);

                let arrResLdmAvanceCG = [], valueArrayResLdmAvanceCG = 0, arrResRevisionLdmAvanceCG = [], arrResCreadorLdmAvanceCG = [],
                arrResModificadorLdmAvanceCG = [], arrResFechaLdmAvanceCG = [], arrResFechaModLdmAvanceCG = [];

                let arrResLdmAvanceTD2 = [], valueArrayResLdmAvanceTD2 = 0, arrResRevisionLdmAvanceTD2 = [], arrResCreadorLdmAvanceTD2 = [],
                arrResModificadorLdmAvanceTD2 = [], arrResFechaLdmAvanceTD2 = [], arrResFechaModLdmAvanceTD2 = [];

                let arrRes80Ldm = [], valueArrayRes80Ldm = 0, arrResRevision80Ldm = [], arrResCreador80Ldm = [],
                arrResModificador80Ldm = [], arrResFecha80Ldm = [], arrResFechaMod80Ldm = [];

                let arrResInfoModelo = [], valueArrayResInfoModelo = 0, arrResRevisionInfoModelo = [], arrResCreadorInfoModelo = [],
                arrResModificadorInfoModelo = [], arrResFechaInfoModelo = [], arrResFechaModInfoModelo = [];

                if (pathMongoOtProject[n].otStatus) { %>
                    <tr name="rowSelected<%=k%>_<%=n%>" style="height: 7vh;">
                        <% for (let q = 12; totalColDataInfo80 > q; q++) {
                            if (q==12) { %>
                                <!-- LDM Avance CG data Activo-->
                                <td class="text-center align-middle">
                                    <% if (largoArrayInfo80 > 0) {

                                        for (let i = 0; largoArrayInfo80 > i; i++) {
                                            if (pathMongoOtInfo80[i]) {
                                                let largoArray = parseInt(largoArrayInfo80)
                                                resLdmAvanceCG = pathMongoOtInfo80[largoArray-1].ldmAvanceCG || 'sinDato'
                                                resRevisionLdmAvanceCG = pathMongoOtInfo80[largoArray-1].revisionLdmAvanceCG || 0
                                                
                                                arrResLdmAvanceCG.push(pathToMongoDBOtInfo80[i].ldmAvanceCG)
                                                arrResRevisionLdmAvanceCG.push(pathToMongoDBOtInfo80[i].revisionLdmAvanceCG)
                                                
                                                arrResCreadorLdmAvanceCG.push(pathToMongoDBOtInfo80[i].creator[0].username)
                                                arrResModificadorLdmAvanceCG.push(pathToMongoDBOtInfo80[i].modificator[0].username)
                                                arrResFechaLdmAvanceCG.push(pathToMongoDBOtInfo80[i].timestamp)
                                                arrResFechaModLdmAvanceCG.push(pathToMongoDBOtInfo80[i].modifiedOn)
                                            }
                                        }
                                        valueArrayResLdmAvanceCG = parseInt(arrResLdmAvanceCG.length-1)
                                            
                                        if (arrResLdmAvanceCG.length == 1 ) {
                                            anteriorDisabled = "disabled"
                                            siguienteDisabled = "disabled"
                                        } else if (arrResLdmAvanceCG.length == arrResRevisionLdmAvanceCG.length) {
                                            anteriorDisabled = ""
                                            siguienteDisabled = "disabled"
                                        } else {
                                            anteriorDisabled = ""
                                            siguienteDisabled = "disabled"
                                        }

                                    } else {
                                        arrResLdmAvanceCG.push(resLdmAvanceCG)
                                        arrResRevisionLdmAvanceCG.push(resRevisionLdmAvanceCG)
                                    } %>

                                    <div id="btnAnteriorSiguienteLdmAvanceCG<%=k%>_<%=n%>_<%=q%>">
                                        <button id="btnAnteriorLdmAvanceCG<%=k%>_<%=n%>_<%=q%>"
                                            name="btnAnteriorLdmAvanceCG" value="<%=k%>_<%=n%>_<%=q%>" class="btn btn-light btn-sm me-2" title="Ver anterior" type="button"
                                            style="--bs-btn-padding-y: .25rem; --bs-btn-padding-x: .35rem; --bs-btn-font-size: .75rem; color: var(--bs-black); cursor: pointer;"
                                            <%= anteriorDisabled %>
                                            ><i class="fa-solid fa-arrow-left"></i>
                                        </button>
                                        <input type="hidden" id="resHidden<%=k%>_<%=n%>_<%=q%>" value="<%= arrResLdmAvanceCG %>">
                                        <input type="hidden" id="resIndexHidden<%=k%>_<%=n%>_<%=q%>" value="<%= valueArrayResLdmAvanceCG %>">
                                        <input type="hidden" id="resRevisionHidden<%=k%>_<%=n%>_<%=q%>" value="<%= arrResRevisionLdmAvanceCG %>">
                                        <input type="hidden" id="arrResCreadorHidden<%=k%>_<%=n%>_<%=q%>" value="<%= arrResCreadorLdmAvanceCG %>">
                                        <input type="hidden" id="arrResModificadorHidden<%=k%>_<%=n%>_<%=q%>" value="<%= arrResModificadorLdmAvanceCG %>">
                                        <input type="hidden" id="arrResFechaHidden<%=k%>_<%=n%>_<%=q%>" value="<%= arrResFechaLdmAvanceCG %>">
                                        <input type="hidden" id="arrResFechaModificacionHidden<%=k%>_<%=n%>_<%=q%>" value="<%= arrResFechaModLdmAvanceCG %>">

                                        <% const config = Info80Config[resLdmAvanceCG] || Info80Config.default; %>
                                        <span id="resLdmAvanceCG<%=k%>_<%=n%>_<%=q%>" class="badge rounded-pill <%= config.classes %> my-2">
                                            <%= config.text %>
                                        </span> |
                                        <span id="resRevisionLdmAvanceCG<%=k%>_<%=n%>_<%=q%>" name="resRevisionLdmAvanceCG" class="badge text-dark my-2" style="cursor: help;">
                                            <%= resRevisionLdmAvanceCG %>
                                        </span>

                                        <button id="btnSiguienteLdmAvanceCG<%=k%>_<%=n%>_<%=q%>"
                                            name="btnSiguienteLdmAvanceCG" value="<%=k%>_<%=n%>_<%=q%>" class="btn btn-light btn-sm ms-1" title="Ver siguiente" type="button"
                                            style="--bs-btn-padding-y: .25rem; --bs-btn-padding-x: .35rem; --bs-btn-font-size: .75rem; color: var(--bs-black); cursor: pointer;"
                                            <%= siguienteDisabled %>
                                            ><i class="fa-solid fa-arrow-right"></i>
                                        </button>
                                    </div>
                                </td>

                            <% } else if (q==13) { %>
                                <!-- LDM Avance TD2 data Activo-->
                                <td class="text-center align-middle">
                                    <% if (largoArrayInfo80 > 0) {

                                        for (let i = 0; largoArrayInfo80 > i; i++) {
                                            if (pathMongoOtInfo80[i]) {
                                                let largoArray = parseInt(largoArrayInfo80)
                                                resLdmAvanceTD2 = pathMongoOtInfo80[largoArray-1].ldmAvanceTD2 || 'sinDato'
                                                resRevisionLdmAvanceTD2 = pathMongoOtInfo80[largoArray-1].revisionLdmAvanceTD2 || 0
                                                
                                                arrResLdmAvanceTD2.push(pathToMongoDBOtInfo80[i].ldmAvanceCG)
                                                arrResRevisionLdmAvanceTD2.push(pathToMongoDBOtInfo80[i].revisionLdmAvanceTD2)
                                                
                                                arrResCreadorLdmAvanceTD2.push(pathToMongoDBOtInfo80[i].creator[0].username)
                                                arrResModificadorLdmAvanceTD2.push(pathToMongoDBOtInfo80[i].modificator[0].username)
                                                arrResFechaLdmAvanceTD2.push(pathToMongoDBOtInfo80[i].timestamp)
                                                arrResFechaModLdmAvanceTD2.push(pathToMongoDBOtInfo80[i].modifiedOn)
                                            }
                                        }
                                        valueArrayResLdmAvanceTD2 = parseInt(arrResLdmAvanceTD2.length-1)

                                        if (arrResLdmAvanceTD2.length == 1 ) {
                                            anteriorDisabled = "disabled"
                                            siguienteDisabled = "disabled"
                                        } else if (arrResLdmAvanceTD2.length == arrResRevisionLdmAvanceTD2.length) {
                                            anteriorDisabled = ""
                                            siguienteDisabled = "disabled"
                                        } else {
                                            anteriorDisabled = ""
                                            siguienteDisabled = "disabled"
                                        }

                                    } else {
                                        arrResLdmAvanceTD2.push(resLdmAvanceTD2)
                                        arrResRevisionLdmAvanceTD2.push(resRevisionLdmAvanceTD2)
                                    } %>

                                    <div id="btnAnteriorSiguienteLdmAvanceTD2<%=k%>_<%=n%>_<%=q%>">
                                        <button id="btnAnteriorLdmAvanceTD2<%=k%>_<%=n%>_<%=q%>"
                                            name="btnAnteriorLdmAvanceTD2" value="<%=k%>_<%=n%>_<%=q%>" class="btn btn-light btn-sm me-2" title="Ver anterior" type="button"
                                            style="--bs-btn-padding-y: .25rem; --bs-btn-padding-x: .35rem; --bs-btn-font-size: .75rem; color: var(--bs-black); cursor: pointer;"
                                            <%= anteriorDisabled %>
                                            ><i class="fa-solid fa-arrow-left"></i>
                                        </button>
                                        <input type="hidden" id="resHidden<%=k%>_<%=n%>_<%=q%>" value="<%= arrResLdmAvanceTD2 %>">
                                        <input type="hidden" id="resIndexHidden<%=k%>_<%=n%>_<%=q%>" value="<%= valueArrayResLdmAvanceTD2 %>">
                                        <input type="hidden" id="resRevisionHidden<%=k%>_<%=n%>_<%=q%>" value="<%= arrResRevisionLdmAvanceTD2 %>">
                                        <input type="hidden" id="arrResCreadorHidden<%=k%>_<%=n%>_<%=q%>" value="<%= arrResCreadorLdmAvanceTD2 %>">
                                        <input type="hidden" id="arrResModificadorHidden<%=k%>_<%=n%>_<%=q%>" value="<%= arrResModificadorLdmAvanceTD2 %>">
                                        <input type="hidden" id="arrResFechaHidden<%=k%>_<%=n%>_<%=q%>" value="<%= arrResFechaLdmAvanceTD2 %>">
                                        <input type="hidden" id="arrResFechaModificacionHidden<%=k%>_<%=n%>_<%=q%>" value="<%= arrResFechaModLdmAvanceTD2 %>">

                                        <% const config = Info80Config[resLdmAvanceTD2] || Info80Config.default; %>
                                        <span id="resLdmAvanceTD2<%=k%>_<%=n%>_<%=q%>" class="badge rounded-pill <%= config.classes %> my-2">
                                            <%= config.text %>
                                        </span> |
                                        <span id="resRevisionLdmAvanceTD2<%=k%>_<%=n%>_<%=q%>" name="resRevisionLdmAvanceTD2" class="badge text-dark my-2" style="cursor: help;">
                                            <%= resRevisionLdmAvanceTD2 %>
                                        </span>
 
                                        <button id="btnSiguienteLdmAvanceTD2<%=k%>_<%=n%>_<%=q%>"
                                            name="btnSiguienteLdmAvanceTD2" value="<%=k%>_<%=n%>_<%=q%>" class="btn btn-light btn-sm ms-1" title="Ver siguiente" type="button"
                                            style="--bs-btn-padding-y: .25rem; --bs-btn-padding-x: .35rem; --bs-btn-font-size: .75rem; color: var(--bs-black); cursor: pointer;"
                                            <%= siguienteDisabled %>
                                            ><i class="fa-solid fa-arrow-right"></i>
                                        </button>
                                    </div>
                                </td>

                            <% } else if (q==14) { %>
                                <!-- LDM 80% data Activo-->
                                <td class="text-center align-middle">
                                    <% if (largoArrayInfo80 > 0) {

                                        for (let i = 0; largoArrayInfo80 > i; i++) {
                                            if (proyecto[m].project[j].oci[k].otProject[n].otInformation[0].otInfoInfo80) {
                                                let largoArray = parseInt(largoArrayInfo80)
                                                res80Ldm = parseInt(pathMongoOtInfo80[largoArray-1].ldm80) || 0
                                                resRevision80Ldm = pathMongoOtInfo80[largoArray-1].revisionLdm80 || 0
                                                
                                                arrRes80Ldm.push(pathToMongoDBOtInfo80[i].ldm80)
                                                arrResRevision80Ldm.push(pathToMongoDBOtInfo80[i].revisionLdm80)
                                                
                                                arrResCreador80Ldm.push(pathToMongoDBOtInfo80[i].creator[0].username)
                                                arrResModificador80Ldm.push(pathToMongoDBOtInfo80[i].modificator[0].username)
                                                arrResFecha80Ldm.push(pathToMongoDBOtInfo80[i].timestamp)
                                                arrResFechaMod80Ldm.push(pathToMongoDBOtInfo80[i].modifiedOn)
                                            }
                                        }
                                        valueArrayRes80Ldm = parseInt(arrRes80Ldm.length-1)

                                        if (arrRes80Ldm.length == 1 ) {
                                            anteriorDisabled = "disabled"
                                            siguienteDisabled = "disabled"
                                        } else if (arrRes80Ldm.length == arrResRevision80Ldm.length) {
                                            anteriorDisabled = ""
                                            siguienteDisabled = "disabled"
                                        } else {
                                            anteriorDisabled = ""
                                            siguienteDisabled = "disabled"
                                        }

                                    } else {
                                        arrRes80Ldm.push(res80Ldm)
                                        arrResRevision80Ldm.push(resRevision80Ldm)
                                    } %>

                                    <div id="btnAnteriorSiguiente80Ldm<%=k%>_<%=n%>_<%=q%>">
                                        <button id="btnAnterior80Ldm<%=k%>_<%=n%>_<%=q%>"
                                            name="btnAnterior80Ldm" value="<%=k%>_<%=n%>_<%=q%>" class="btn btn-light btn-sm me-1" title="Ver anterior" type="button"
                                            style="--bs-btn-padding-y: .25rem; --bs-btn-padding-x: .35rem; --bs-btn-font-size: .75rem; color: var(--bs-black); cursor: pointer;"
                                            <%= anteriorDisabled %>
                                            ><i class="fa-solid fa-arrow-left"></i>
                                        </button>
                                        <input type="hidden" id="resHidden<%=k%>_<%=n%>_<%=q%>" value="<%= arrRes80Ldm %>">
                                        <input type="hidden" id="resIndexHidden<%=k%>_<%=n%>_<%=q%>" value="<%= valueArrayRes80Ldm %>">
                                        <input type="hidden" id="resRevisionHidden<%=k%>_<%=n%>_<%=q%>" value="<%= arrResRevision80Ldm %>">
                                        <input type="hidden" id="arrResCreadorHidden<%=k%>_<%=n%>_<%=q%>" value="<%= arrResCreador80Ldm %>">
                                        <input type="hidden" id="arrResModificadorHidden<%=k%>_<%=n%>_<%=q%>" value="<%= arrResModificador80Ldm %>">
                                        <input type="hidden" id="arrResFechaHidden<%=k%>_<%=n%>_<%=q%>" value="<%= arrResFecha80Ldm %>">
                                        <input type="hidden" id="arrResFechaModificacionHidden<%=k%>_<%=n%>_<%=q%>" value="<%= arrResFechaMod80Ldm %>">

                                        <span id="res80Ldm<%=k%>_<%=n%>_<%=q%>" name="res80Ldm" class="text-dark" style="font-size: .9rem;">
                                            <%= res80Ldm %>%
                                        </span>
                                        <% if (res80Ldm == 100) { %>
                                            <i class="fa-solid fa-circle-check fa-lg" style="color: #067e00;"></i>
                                        <% } else if (res80Ldm == 0) { %>
                                            <i class="fa-solid fa-spinner fa-lg fa-spin-pulse" style="color: #970000f5;"></i>
                                        <% } else { %>
                                            <i class="fa-solid fa-person-digging fa-lg" style="color: #080183;"></i>
                                        <% } %>
                                        |
                                        <span id="resRevision80Ldm<%=k%>_<%=n%>_<%=q%>" name="resRevision80Ldm" class="badge text-dark my-2" style="font-size: .9rem;">
                                            <%= resRevision80Ldm %>
                                        </span>
                                        
                                        <button id="btnSiguiente80Ldm<%=k%>_<%=n%>_<%=q%>"
                                            name="btnSiguiente80Ldm" value="<%=k%>_<%=n%>_<%=q%>" class="btn btn-light btn-sm ms-1" title="Ver siguiente" type="button"
                                            style="--bs-btn-padding-y: .25rem; --bs-btn-padding-x: .35rem; --bs-btn-font-size: .75rem; color: var(--bs-black); cursor: pointer;"
                                            <%= siguienteDisabled %>
                                            ><i class="fa-solid fa-arrow-right"></i>
                                        </button>
                                    </div>
                                </td>

                            <% } else { %>
                                <!-- Info Modelo data Activo-->
                                <td class="text-center align-middle">
                                    <% if (largoArrayInfo80 > 0) {

                                        for (let i = 0; largoArrayInfo80 > i; i++) {
                                            if (pathToMongoDBOtInfo80[i]) {
                                                let largoArray = parseInt(largoArrayInfo80)
                                                resInfoModelo = parseInt(pathToMongoDBOtInfo80[largoArray-1].infoModelo) || 0
                                                resRevisionInfoModelo = parseInt(pathToMongoDBOtInfo80[largoArray-1].revisionInfoModelo) || 0
                                                
                                                arrResInfoModelo.push(pathToMongoDBOtInfo80[i].infoModelo)
                                                arrResRevisionInfoModelo.push(pathToMongoDBOtInfo80[i].revisionInfoModelo)
                                                
                                                arrResCreadorInfoModelo.push(pathToMongoDBOtInfo80[i].creator[0].username)
                                                arrResModificadorInfoModelo.push(pathToMongoDBOtInfo80[i].modificator[0].username)
