                var valueArrayResHorasProceso3d = 0;
                var arrResRevisionHorasProceso3d = [];
                var arrResCreadorHorasProceso3d = [];
                var arrResModificadorHorasProceso3d = [];
                var arrResFechaHorasProceso3d = [];
                var arrResFechaModHorasProceso3d = [];

