                                                        arrResHorasProceso3d.push(pathToMongoDBOtInfoProceso3d.horasProceso3d)
                                                        arrResRevisionHorasProceso3d.push(pathToMongoDBOtInfoProceso3d.revisionHorasProceso3d)
