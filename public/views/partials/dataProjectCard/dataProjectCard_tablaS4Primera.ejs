                                            style="--bs-btn-padding-y: .25rem; --bs-btn-padding-x: .35rem; --bs-btn-font-size: .75rem; color: var(--bs-black); cursor: pointer;"
                                            <%= anteriorDisabled %>
                                            ><i class="fa-solid fa-arrow-left"></i>
                                        </button>
                                        <input type="hidden" id="resHidden<%=k%>_<%=n%>_<%=q%>" value="<%= arrResMas10Menos %>">
                                        <input type="hidden" id="resIndexHidden<%=k%>_<%=n%>_<%=q%>" value="<%= valueArrayResMas10Menos %>">
                                        <input type="hidden" id="resRevisionHidden<%=k%>_<%=n%>_<%=q%>" value="<%= arrResRevisionMas10Menos %>">
                                        <input type="hidden" id="arrResCreadorHidden<%=k%>_<%=n%>_<%=q%>" value="<%= arrResCreadorMas10Menos %>">
                                        <input type="hidden" id="arrResModificadorHidden<%=k%>_<%=n%>_<%=q%>" value="<%= arrResModificadorMas10Menos %>">
                                        <input type="hidden" id="arrResFechaHidden<%=k%>_<%=n%>_<%=q%>" value="<%= arrResFechaMas10Menos %>">
                                        <input type="hidden" id="arrResFechaModificacionHidden<%=k%>_<%=n%>_<%=q%>" value="<%= arrResFechaModMas10Menos %>">
        
                                        <% const config = sim4PrimeraConfig[resMas10Menos] || sim4PrimeraConfig.default; %>
                                        <span id="resMas10Menos<%=k%>_<%=n%>_<%=q%>" class="badge rounded-pill <%= config.classes %> my-2" >
                                            <%= config.text %>
                                        </span>
                                        |
                                        <span id="resRevisionMas10Menos<%=k%>_<%=n%>_<%=q%>" name="resRevisionMas10Menos" class="badge text-dark my-2" style="cursor: help;">
                                            <%= resRevisionMas10Menos %>
                                        </span>
                                        
                                        <button id="btnSiguienteMas10Menos<%=k%>_<%=n%>_<%=q%>"
                                            name="btnSiguienteMas10Menos" value="<%=k%>_<%=n%>_<%=q%>" class="btn btn-light btn-sm ms-2" title="Ver siguiente" type="button"
                                            style="--bs-btn-padding-y: .25rem; --bs-btn-padding-x: .35rem; --bs-btn-font-size: .75rem; color: var(--bs-black); cursor: pointer;"
                                            <%= siguienteDisabled %>
                                            ><i class="fa-solid fa-arrow-right"></i>
                                        </button>
                                    </div>
                                </td>

                            <% } else if (q==31) { %>
                                <!-- MpAlternativo Sim1 data Activo-->
                                <td class="text-center align-middle">
                                    <% if (largoArraySim4Primera > 0) {
                                        
                                        for (let i = 0; largoArraySim4Primera > i; i++) {
                                            if (pathMongoOtSim4Primera[i]) {
                                                let largoArray = parseInt(largoArraySim4Primera)
                                                resMpAlternativo = pathMongoOtSim4Primera[largoArray-1].mpAlternativo || 'sinDato'
                                                resRevisionMpAlternativo = parseInt(pathMongoOtSim4Primera[largoArray-1].revisionMpAlternativo) || 0
                                                
                                                arrResMpAlternativo.push(pathMongoOtSim4Primera[i].mpAlternativo)
                                                arrResRevisionMpAlternativo.push(pathMongoOtSim4Primera[i].revisionMpAlternativo)
                                            
                                                arrResCreadorMpAlternativo.push(pathMongoOtSim4Primera[i].creator[0].username)
                                                arrResModificadorMpAlternativo.push(pathMongoOtSim4Primera[i].modificator[0].username)
                                                arrResFechaMpAlternativo.push(pathMongoOtSim4Primera[i].timestamp)
                                                arrResFechaModMpAlternativo.push(pathMongoOtSim4Primera[i].modifiedOn)
                                            }
                                        }
                                        valueArrayResMpAlternativo = parseInt(arrResMpAlternativo.length-1)
                                        
                                        if (arrResMpAlternativo.length == 1 ) {
                                            siguienteDisabled = "disabled"
                                            anteriorDisabled = "disabled"
                                        } else if (arrResMpAlternativo.length == arrResRevisionMpAlternativo.length) {
                                            anteriorDisabled = ""
                                            siguienteDisabled = "disabled"
                                        } else {
                                            anteriorDisabled = ""
                                            siguienteDisabled = "disabled"
                                        }
                                        
                                    } else {
                                        arrResMpAlternativo.push(resMpAlternativo)
                                        arrResRevisionMpAlternativo.push(resRevisionMpAlternativo)

                                        arrResCreadorMpAlternativo.push(pathMongoOtProject[n].creator[0].username)
                                        arrResModificadorMpAlternativo.push(pathMongoOtProject[n].modificator[0].username)
                                        arrResFechaMpAlternativo.push(pathMongoOtProject[n].timestamp)
                                        arrResFechaModMpAlternativo.push(pathMongoOtProject[n].modifiedOn)
                                    } %>
                                    
                                    <div id="btnAnteriorSiguienteMpAlternativo<%=k%>_<%=n%>_<%=q%>">
                                        <button id="btnAnteriorMpAlternativo<%=k%>_<%=n%>_<%=q%>"
                                            name="btnAnteriorMpAlternativo" value="<%=k%>_<%=n%>_<%=q%>" class="btn btn-light btn-sm me-2" title="Ver anterior" type="button"
                                            style="--bs-btn-padding-y: .25rem; --bs-btn-padding-x: .35rem; --bs-btn-font-size: .75rem; color: var(--bs-black); cursor: pointer;"
                                            <%= anteriorDisabled %>
                                            ><i class="fa-solid fa-arrow-left"></i>
                                        </button>
                                        <input type="hidden" id="resHidden<%=k%>_<%=n%>_<%=q%>" value="<%= arrResMpAlternativo %>">
                                        <input type="hidden" id="resIndexHidden<%=k%>_<%=n%>_<%=q%>" value="<%= valueArrayResMpAlternativo %>">
                                        <input type="hidden" id="resRevisionHidden<%=k%>_<%=n%>_<%=q%>" value="<%= arrResRevisionMpAlternativo %>">
                                        <input type="hidden" id="arrResCreadorHidden<%=k%>_<%=n%>_<%=q%>" value="<%= arrResCreadorMpAlternativo %>">
                                        <input type="hidden" id="arrResModificadorHidden<%=k%>_<%=n%>_<%=q%>" value="<%= arrResModificadorMpAlternativo %>">
                                        <input type="hidden" id="arrResFechaHidden<%=k%>_<%=n%>_<%=q%>" value="<%= arrResFechaMpAlternativo %>">
                                        <input type="hidden" id="arrResFechaModificacionHidden<%=k%>_<%=n%>_<%=q%>" value="<%= arrResFechaModMpAlternativo %>">
        
                                        <% const config = sim4PrimeraConfig[resMpAlternativo] || sim4PrimeraConfig.default; %>
                                        <span id="resMpAlternativo<%=k%>_<%=n%>_<%=q%>" class="badge rounded-pill <%= config.classes %> my-2" >
                                            <%= config.text %>
                                        </span>
                                        |
                                        <span id="resRevisionMpAlternativo<%=k%>_<%=n%>_<%=q%>" name="resRevisionMpAlternativo" class="badge text-dark my-2" style="cursor: help;">
                                            <%= resRevisionMpAlternativo %>
                                        </span>
                                                    
                                        <button id="btnSiguienteMpAlternativo<%=k%>_<%=n%>_<%=q%>"
                                            name="btnSiguienteMpAlternativo" value="<%=k%>_<%=n%>_<%=q%>" class="btn btn-light btn-sm ms-2" title="Ver siguiente" type="button"
                                            style="--bs-btn-padding-y: .25rem; --bs-btn-padding-x: .35rem; --bs-btn-font-size: .75rem; color: var(--bs-black); cursor: pointer;"
                                            <%= siguienteDisabled %>
                                            ><i class="fa-solid fa-arrow-right"></i>
